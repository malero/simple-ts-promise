    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.5.1",
    "typescript": "^4.2.4"
  },
  "homepage": "https://github.com/malero/simple-ts-promise",
  "git": "https://github.com/malero/simple-ts-promise",
